{
    "name": "johncarlson21/ebay-sdk-php",
    "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
    "keywords": ["ebay", "api", "sdk", "PHP"],
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "johncarlson21",
            "email": "john@surethingweb.com"
        }
    ],
    "support": {
        "issues": "https://github.com/fshch/ebay-sdk-php/issues"
    },
    "require": {
        "php": ">=7.1",
        "myclabs/php-enum": ">=1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "DTS\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DTS\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}
